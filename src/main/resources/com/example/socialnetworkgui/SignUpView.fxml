<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgui.controllers.SignUpController">
   <children>
      <Label layoutX="178.0" layoutY="68.0" text="First Name" textAlignment="JUSTIFY" />
      <TextField fx:id="firstName" layoutX="276.0" layoutY="64.0" />
      <TextField fx:id="lastName" layoutX="277.0" layoutY="107.0" />
      <TextField fx:id="age" layoutX="278.0" layoutY="194.0" />
      <Label layoutX="179.0" layoutY="111.0" text="Last Name" />
      <Label layoutX="196.0" layoutY="198.0" text="Age" />
      <PasswordField fx:id="password" layoutX="278.0" layoutY="151.0" />
      <Label layoutX="179.0" layoutY="155.0" text="Password" />
      <Button layoutX="277.0" layoutY="249.0" mnemonicParsing="false" onAction="#signUpClick" prefHeight="26.0" prefWidth="150.0" text="Sign Up" />
      <Button fx:id="loginButton" layoutX="277.0" layoutY="297.0" mnemonicParsing="false" onAction="#loginClick" prefHeight="26.0" prefWidth="150.0" text="Already have account?" />
      <Button fx:id="duplicateButton" layoutX="277.0" layoutY="344.0" mnemonicParsing="false" onAction="#duplicateClicked" prefHeight="26.0" prefWidth="150.0" text="Duplicate session" />
   </children>
</AnchorPane>
