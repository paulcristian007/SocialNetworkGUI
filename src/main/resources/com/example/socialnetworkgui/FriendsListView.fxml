<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkgui.controllers.FriendsListController">
   <children>
      <ListView fx:id="friendsListView" layoutX="68.0" layoutY="72.0" onMouseClicked="#friendsListClicked" prefHeight="200.0" prefWidth="343.0" />
      <Button fx:id="removeFriendButton" disable="true" layoutX="80.0" layoutY="312.0" mnemonicParsing="false" onAction="#removeFriendClicked" prefHeight="26.0" prefWidth="96.0" text="Remove Friend" />
      <Button fx:id="backButton" layoutX="201.0" layoutY="312.0" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="26.0" prefWidth="96.0" text="Back" />
      <Button fx:id="openChatButton" disable="true" layoutX="314.0" layoutY="312.0" mnemonicParsing="false" onAction="#openChatClicked" prefHeight="26.0" prefWidth="96.0" text="Open Chat" />
   </children>
</AnchorPane>
